#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/melwm/Bootcamp/module_10_Object_Oriented_Programming_OOP/node_modules/esprima/bin/node_modules:/mnt/c/Users/melwm/Bootcamp/module_10_Object_Oriented_Programming_OOP/node_modules/esprima/node_modules:/mnt/c/Users/melwm/Bootcamp/module_10_Object_Oriented_Programming_OOP/node_modules:/mnt/c/Users/melwm/Bootcamp/node_modules:/mnt/c/Users/melwm/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/melwm/Bootcamp/module_10_Object_Oriented_Programming_OOP/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/melwm/Bootcamp/module_10_Object_Oriented_Programming_OOP/node_modules/esprima/bin/node_modules:/mnt/c/Users/melwm/Bootcamp/module_10_Object_Oriented_Programming_OOP/node_modules/esprima/node_modules:/mnt/c/Users/melwm/Bootcamp/module_10_Object_Oriented_Programming_OOP/node_modules:/mnt/c/Users/melwm/Bootcamp/node_modules:/mnt/c/Users/melwm/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/melwm/Bootcamp/module_10_Object_Oriented_Programming_OOP/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../esprima/bin/esvalidate.js" "$@"
fi
